{
  "name": "skyrim-extractor",
  "version": "1.0.0",
  "description": "Skyrim ESP/ESM plugin parser",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "build": "tsc",
    "start": "node dist/index.js",
    "sim": "npm start -- --modlist ../data/mod-samples",
    "watch": "tsc --watch",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'tsc && node dist/index.js' -- --modlist ../data/mod-samples"
  },
  "keywords": [
    "skyrim",
    "esp",
    "esm",
    "parser",
    "modding"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.10",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "fp-ts": "^2.16.2",
    "zod": "^3.22.4"
  }
}
